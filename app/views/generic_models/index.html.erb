<p id="notice"><%= notice %></p>

<h1>Generic Models</h1>

<table>
  <thead>
    <tr>
      <th>User</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @generic_models.each do |generic_model| %>
      <tr>
        <td><%= generic_model.user %></td>
        <td><%= link_to 'Show', generic_model %></td>
        <td><%= link_to 'Edit', edit_generic_model_path(generic_model) %></td>
        <td><%= link_to 'Destroy', generic_model, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Generic Model', new_generic_model_path %>
